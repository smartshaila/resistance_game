<h1>Listing team_votes</h1>

<table>
  <thead>
    <tr>
      <th>Approve</th>
      <th>Team</th>
      <th>Player assignment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_votes.each do |team_vote| %>
      <tr>
        <td><%= team_vote.approve %></td>
        <td><%= team_vote.team %></td>
        <td><%= team_vote.player_assignment %></td>
        <td><%= link_to 'Show', team_vote %></td>
        <td><%= link_to 'Edit', edit_team_vote_path(team_vote) %></td>
        <td><%= link_to 'Destroy', team_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team vote', new_team_vote_path %>
